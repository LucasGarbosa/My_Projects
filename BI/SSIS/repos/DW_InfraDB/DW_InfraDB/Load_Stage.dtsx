                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=15.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">SELECT Distinct
	 o.ObjectId
	,o.HostId
	,o.HWProfileId
	,i.VMInstanceId
	,o.Name
	,o.ObjectState
	,os.Name as OperatingSystem
	,i.VMResourceGroup
	,i.VMResource
	,STUFF((SELECT
		', ' + t2.IPv4Addresses
			FROM  tbl_WLC_VNetworkAdapter t2
		    WHERE o.HWProfileId = t2.HWProfileId 
		    ORDER BY t2.IPv4Addresses
		    FOR XML PATH(''), TYPE).value('.','varchar(max)'),1,2, ''
			) AS IPv4Address
	,na.IPv4AddressType
	,na.VirtualNetwork
	,na.MACAddressType
	,STUFF((SELECT
		', ' + t2.DNSServers
			FROM  tbl_WLC_VNetworkAdapter t2
		    WHERE o.HWProfileId = t2.HWProfileId 
		    ORDER BY t2.DNSServers
		    FOR XML PATH(''), TYPE).value('.','varchar(max)'),1,2, ''
	) AS DNSServers
	,i.ComputerName as DNSName
	,o.Accessibility
	,p.ProcessorCount
	,p.RAM
	,p.MaxRAM
	,p.MemoryWeight
	,p.CPUReserve
	,p.CPUMax
	,p.CPUPerVirtualNumaNodeMaximum
	,p.MemoryPerVirtualNumaNodeMaximumMB
	,p.DynamicMemoryMinimumMB
	,i.VMTotalSize
	,i.VMTotalMaxSize
	,i.CurrentAvailableMemoryPercent 
	,i.AvailableMemoryBuffer
	,o.CreationTime
FROM tbl_WLC_VObject o
	INNER JOIN tbl_WLC_HWProfile p
	ON o.HWProfileId = p.HWProfileId
	INNER JOIN tbl_WLC_VMInstance i
	ON o.ObjectId = i.ObjectId
	INNER JOIN tbl_WLC_VNetworkAdapter na
	ON o.HWProfileId = na.HWProfileId
	INNER JOIN tbl_IL_OS os
	ON o.OSID = os.OSId
